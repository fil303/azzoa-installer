{
  "name": "fil303/azzoa-installer",
  "description": "Laravel web installer",
  "license": "MIT",
  "authors": [
    {
      "name": "Israfil Hossain",
      "email": "webfil303@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0"
  },
  "autoload": {
    "psr-4": {
      "IsrafilHossain\\LaravelInstaller\\": "src/"
    },
    "files": [
      "src/Helpers/functions.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "IsrafilHossain\\LaravelInstaller\\Providers\\LaravelInstallerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
